<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Cliente.
    -->
    <changeSet id="20190412061011-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cliente_proprietario_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_cliente_proprietario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente_proprietario"/>

        <addForeignKeyConstraint baseColumnNames="cliente_comprador_id"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente_cliente_comprador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente_comprador"/>

    </changeSet>
</databaseChangeLog>
